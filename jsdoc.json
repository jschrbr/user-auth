{
  "source": {
    "include": ["./"],
    "includePattern": ".js$",
    "excludePattern": "node_modules/"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "readme": "./DOC.md",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme"
  }
}
